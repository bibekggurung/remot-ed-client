  <section data-id="{{classroom._id}}" class="single-classroom">
    <h1>{{classroom.classname}}</h1>

    <table class="table table-striped table-bordered survey-list" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th class="th th-title">Quizzes</th>
          <th class="th">Time</th>
          <th class="th">Score</th>
        </tr>
      </thead>
      <tbody>
    {{#each classroom.quizzes as |quiz|}}
        <tr>
          <td class="td-title col-6 single-question-link" data-id="{{quiz._id}}"> {{quiz.title}} </td>
          <td class="td-title col-6 single-question-link" data-id="{{quiz._id}}"> {{quiz.title}} </td>
          <td class="td-title col-6 single-question-link" data-id="{{quiz._id}}"> {{quiz.title}} </td>
        </tr>
    {{/each}}
    </tbody>
  </table>

    <button class="btn btn-secondary class-to-teacher-dash"> Back to Teacher Dash </button>
</section>
