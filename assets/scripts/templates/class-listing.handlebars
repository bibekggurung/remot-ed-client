  <table class="table table-striped table-bordered table-sm survey-list" cellspacing="0" width="100%">
    <thead>
      <tr>
        <th class="th-sm th-title">Title</th>
        <th class="th-sm th-title">Students</th>
        <th class="th-sm th-action">Edit</th>
      </tr>
    </thead>
    <tbody>

  {{#each classrooms as |class|}}
    <section data-id="{{class._id}}">
      <tr>
          <td class="td-title">{{class.classname}}</td>
          <td class="td-title">{{class.students.length}} </td>
          <td>
          <button data-id="{{question._id}}" data-toggle="modal" title="see result" class="btn btn-info see-results">
            <svg id="i-clipboard" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="20" height="20" fill="none" stroke="currentcolor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2">
                  <path d="M12 2 L12 6 20 6 20 2 12 2 Z M11 4 L6 4 6 30 26 30 26 4 21 4" />
                </svg>
              </button>
        </td>
        </tr>
          <!-- <button data-id="{{question._id}}" title="Show" class="btn btn-info get-question-btn">
            SHOW
          </button> -->

    </section>
  {{/each}}
  </tbody>
  </table>
