  <section data-id="{{classroom._id}}" class="single-classroom">
    <p>{{classroom.title}}</p>

    <table class="table table-striped table-bordered survey-list" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th class="th th-title">Questions</th>
          <th class="th"></th>
          <th class="th"></th>
        </tr>
      </thead>
      <tbody>
    {{#each classroom.students as |student|}}
      <section data-id="{{student._id}}">
        <tr>
          <td class="td-title col-6 single-question-link" data-id="{{student._id}}" >{{student.email}}</td>
        </tr>
      </section>
    {{/each}}
    <button class="btn btn-secondary quiz-to-teacher-dash">Back to Teacher Dash</button>
  </section>
