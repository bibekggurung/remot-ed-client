  <section data-id="{{classroom._id}}" class="single-classroom">
    <h1>{{classroom.classname}}</h1>

    <table class="table table-striped table-bordered survey-list" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th class="th th-title">"Name"</th>
          <th class="th"></th>
          <th class="th"></th>
        </tr>
      </thead>
      <tbody>
    {{#each classroom.students as |student|}}
        <tr>
          <td class="td-title col-6 single-question-link" data-id="{{student._id}}" >{{student._id}}</td>
        </tr>
    {{/each}}
    <button class="btn btn-secondary quiz-to-teacher-dash">Back to Teacher Dash</button>
    <button data-id="{{classroom._id}}" class="btn btn-info edit"> Edit </button>
    <button data-id="{{classroom._id}}" class="btn btn-danger delete"> DELETE </button>
</section>
